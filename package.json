    "@usedapp/core": "^1.0.15",
    "@walletconnect/web3-provider": "^1.7.8",
    "ethers": "^5.6.9",
    "formact": "^4.0.4",
    "framer-motion": "^6.3.16",
    "moment": "^2.29.3",
    "next": "12.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^4.4.0"
  },
  "devDependencies": {
    "@typechain/ethers-v5": "^10.1.0",
    "@types/moment": "^2.13.0",
    "@types/node": "18.0.0",
