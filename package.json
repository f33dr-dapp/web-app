  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "typechain": "typechain --target ethers-v5 --out-dir contracts '../smart-contracts/build/contracts/*.json' && npm run manifest && npm run hooks",
